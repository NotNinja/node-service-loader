{
  "extends": "notninja/es6",
  "env": {
    "node": true
  },
  "rules": {
    "func-names": [
      "error",
      "always"
    ],
    "global-require": "off",
    "max-params": [
      "error",
      4
    ],
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ]
  }
}
